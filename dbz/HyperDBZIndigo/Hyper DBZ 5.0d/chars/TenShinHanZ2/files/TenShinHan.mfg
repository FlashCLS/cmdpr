ID = 90000210
removeTime = 3 + IfElse(HitPauseTime, HitPauseTime - 1, 0)
pauseMoveTime = 3 + IfElse(HitPauseTime, HitPauseTime - 1, 0)
superMoveTime = 3 + IfElse(HitPauseTime, HitPauseTime - 1, 0)
